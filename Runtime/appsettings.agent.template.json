{
    "TaskAgentConfiguration": {
        "HttpClientTimeoutSupplement": 0
    },
    "OpenId": {
        "OpenIdClients": {
            "Job": "<NonHashedPasswordOfJobRemote>"
        },
        "DefaultOpenIdClient": "Job"
    },
    "PasswordResetSettings": {
        "TwoFactorSettings": {
            "ApplicationUri": "<UrlServer>"
        },
        "NotificationSettings": {
            "Cultures": [
                "en"
            ]
        },
        "EncryptionCertificate": {
            "File": "<EncryptionCertificatePath>",
            "Password": "<EncryptionCertificatePassword>"
        },
        "TokenBuildingSettings": {
            "ValidFor": "03:00:00"
        },
        "MailSettings": {
            "PickupDirectory": "<MailFolder>",
            "FromAddress": "no-reply@<Client>.com",
            "UseSpecifiedPickupDirectory": true
        }
    },
    "SourcesRootPaths": [
        "<SourcesFolder>"
    ],
    "UsercubeServer": {
        "Url": "<UrlServer>"
    },
    "UsercubeAgent": {
        "Url": "<UrlAgent>"
    },
    "Connections": {}
}